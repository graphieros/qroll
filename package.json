{
  "name": "qroll",
  "version": "0.0.3",
  "private": false,
  "description": "A friendly full page scroll UI library",
  "author": "Alec Lloyd Probert",
  "licence": "MIT",
  "type": "module",
  "main": "./dist/qroll.umd.cjs",
  "module": "./dist/qroll.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphieros/qroll"
  },
  "exports": {
    ".": {
      "import": "./dist/qroll.js",
      "export": "./dist/qroll.umd.cjs"
    }
  },
  "keywords": [
    "UI library",
    "full page",
    "scroll",
    "scroll library",
    "slider",
    "slides",
    "slide library"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "dev:build": "vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.1",
    "@types/node": "^14.18.36",
    "@vitest/coverage-c8": "^0.28.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^1.7.2",
    "vitest": "^0.28.4"
  }
}
